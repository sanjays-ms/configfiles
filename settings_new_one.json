  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Basic text & variable names",
        "scope": [
          "text",
          "source",
          "variable.other.readwrite",
          "punctuation.definition.variable"
        ],
        "settings": {
          "foreground": "#cdd6f4"
        }
      },
      {
        "name": "Comments",
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "foreground": "#6c7086",
          "fontStyle": ""
        }
      },
      {
        "name": "Strings",
        "scope": [
          "string",
          "punctuation.definition.string"
        ],
        "settings": {
          "foreground": "#a6e3a1",
        }
      },
      {
        "name": "String escapes",
        "scope": [
          "constant.character.escape",
          "constant.other.placeholder"
        ],
        "settings": {
          "foreground": "#f5c2e7"
        }
      },
      {
        "name": "Numbers and constants",
        "scope": [
          "constant.numeric",
          "constant.language",
          "constant.language.boolean",
          "constant.language.null",
          "constant.language.undefined",
          "variable.other.constant",
          "entity.name.constant"
        ],
        "settings": {
          "foreground": "#fab387"
        }
      },
      {
        "name": "Keywords",
        "scope": [
          "keyword",
          "keyword.control",
          "keyword.operator.word",
          "keyword.operator.new",
          "storage.type",
          "storage.modifier",
          "variable.language.this",
          "variable.language.super",
          "punctuation.definition.keyword"
        ],
        "settings": {
          "foreground": "#cba6f7",
        }
      },
      {
        "name": "Operators",
        "scope": [
          "keyword.operator",
          "punctuation.accessor",
          "punctuation.separator",
          "punctuation.terminator",
          "meta.brace"
        ],
        "settings": {
          "foreground": "#89dceb"
        }
      },
      {
        "name": "Functions",
        "scope": [
          "entity.name.function",
          "meta.function-call",
          "support.function",
          "variable.function"
        ],
        "settings": {
          "foreground": "#89b4fa"
        }
      },
      {
        "name": "Classes and types",
        "scope": [
          "entity.name.class",
          "entity.name.type",
          "entity.other.inherited-class",
          "support.class",
          "support.type",
          "entity.name.namespace",
          "entity.name.module"
        ],
        "settings": {
          "foreground": "#f9e2af"
        }
      },
      {
        "name": "Interfaces",
        "scope": [
          "entity.name.interface",
          "entity.name.trait"
        ],
        "settings": {
          "foreground": "#f9e2af"
        }
      },
      {
        "name": "Enums",
        "scope": [
          "entity.name.enum"
        ],
        "settings": {
          "foreground": "#f9e2af"
        }
      },
      {
        "name": "Enum members",
        "scope": [
          "variable.other.enummember",
          "constant.other.enum"
        ],
        "settings": {
          "foreground": "#94e2d5"
        }
      },
      {
        "name": "Parameters",
        "scope": [
          "variable.parameter",
          "meta.function.parameters"
        ],
        "settings": {
          "foreground": "#eba0ac"
        }
      },
      {
        "name": "Object properties",
        "scope": [
          "variable.other.property",
          "meta.property.object",
          "entity.name.tag.yaml",
          "support.type.property-name"
        ],
        "settings": {
          "foreground": "#89b4fa"
        }
      },
      {
        "name": "Decorators",
        "scope": [
          "meta.decorator",
          "meta.annotation",
          "punctuation.decorator",
          "punctuation.definition.decorator",
          "storage.type.annotation"
        ],
        "settings": {
          "foreground": "#f5c2e7"
        }
      },
      {
        "name": "Regular expressions",
        "scope": [
          "string.regexp",
          "constant.regexp",
          "constant.character.character-class.regexp",
          "constant.other.character-class.regexp",
          "constant.other.character-class.set.regexp",
          "punctuation.definition.character-class.regexp"
        ],
        "settings": {
          "foreground": "#f5c2e7"
        }
      },
      {
        "name": "Tags",
        "scope": [
          "entity.name.tag",
          "punctuation.definition.tag"
        ],
        "settings": {
          "foreground": "#f38ba8"
        }
      },
      {
        "name": "Attributes",
        "scope": [
          "entity.other.attribute-name",
          "entity.other.attribute"
        ],
        "settings": {
          "foreground": "#f9e2af"
        }
      },
      {
        "name": "Support",
        "scope": [
          "support.constant",
          "support.variable"
        ],
        "settings": {
          "foreground": "#fab387"
        }
      },
      {
        "name": "Invalid",
        "scope": [
          "invalid",
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "#FFFFFF",
          //"fontStyle": "bold underline"
        }
      },
      {
        "name": "Markup headings",
        "scope": [
          "markup.heading",
          "punctuation.definition.heading"
        ],
        "settings": {
          "foreground": "#89b4fa",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup bold",
        "scope": [
          "markup.bold"
        ],
        "settings": {
          "foreground": "#fab387",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup italic",
        "scope": [
          "markup.italic"
        ],
        "settings": {
          "foreground": "#fab387",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Markup code",
        "scope": [
          "markup.inline.raw",
          "markup.raw",
          "markup.fenced_code"
        ],
        "settings": {
          "foreground": "#a6e3a1"
        }
      },
      {
        "name": "Markup links",
        "scope": [
          "markup.underline.link",
          "string.other.link"
        ],
        "settings": {
          "foreground": "#89b4fa",
          "fontStyle": "underline"
        }
      },
      {
        "name": "Markup lists",
        "scope": [
          "markup.list",
          "punctuation.definition.list"
        ],
        "settings": {
          "foreground": "#f38ba8"
        }
      },
      {
        "name": "Markup quotes",
        "scope": [
          "markup.quote"
        ],
        "settings": {
          "foreground": "#9399b2"
        }
      },
      {
        "name": "Punctuation",
        "scope": [
          "punctuation",
          "meta.brace.round",
          "meta.brace.square",
          "meta.brace.curly",
          "punctuation.section",
          "punctuation.definition.parameters",
          "punctuation.definition.arguments"
        ],
        "settings": {
          "foreground": "#7f849c"
        }
      },
      {
        "name": "Storage modifiers",
        "scope": [
          "storage.modifier.async",
          "storage.modifier.const",
          "storage.modifier.static",
          "storage.modifier.readonly"
        ],
        "settings": {
          "foreground": "#cba6f7"
        }
      },
      {
        "name": "Special variables",
        "scope": [
          "variable.language",
          "variable.other.predefined"
        ],
        "settings": {
          "foreground": "#cba6f7"
        }
      },
      {
        "name": "Diff added",
        "scope": [
          "markup.inserted",
          "meta.diff.header.to-file"
        ],
        "settings": {
          "foreground": "#a6e3a1"
        }
      },
      {
        "name": "Diff removed",
        "scope": [
          "markup.deleted",
          "meta.diff.header.from-file"
        ],
        "settings": {
          "foreground": "#f38ba8"
        }
      },
      {
        "name": "Diff changed",
        "scope": [
          "markup.changed"
        ],
        "settings": {
          "foreground": "#f9e2af"
        }
      },
    ]
  },
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "update.showReleaseNotes": false,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "dotnet.automaticallyCreateSolutionInWorkspace": false,
  "workbench.iconTheme": "material-icon-theme",
}

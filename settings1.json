    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Comment",
                "scope": [
                    "comment",
                    "comment.block.documentation",
                    "punctuation.definition.comment"
                ],
                "settings": {
                    "foreground": "#5c6370"
                }
            },
            {
                "name": "String",
                "scope": [
                    "string",
                    "string.quoted",
                    "string.template"
                ],
                "settings": {
                    // "foreground": "#98c379"
                    "foreground": "#aca29b"
                }
            },
            {
                "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
                "scope": [
                    "constant.numeric",
                    "constant.language",
                    "support.constant",
                    "constant.character",
                    "variable.parameter"
                ],
                "settings": {
                    "foreground": "#d19a66"
                }
            },
            {
                "name": "Keyword, Storage",
                "scope": [
                    "keyword",
                    "storage.type",
                    "storage.modifier"
                ],
                "settings": {
                    "foreground": "#c678dd"
                }
            },
            {
                "name": "Entity Types",
                "scope": [
                    "support.type",
                    "support.class",
                    "entity.name.type",
                    "entity.name.class"
                ],
                "settings": {
                    "foreground": "#e5c07b"
                }
            },
            {
                "name": "Function, Special Method, Block Level",
                "scope": [
                    "entity.name.function",
                    "meta.function-call",
                    "variable.function",
                    "support.function"
                ],
                "settings": {
                    "foreground": "#61afef"
                }
            },
            {
                "name": "Variables",
                "scope": [
                    "variable",
                    "support.variable"
                ],
                "settings": {
                    "foreground": "#e06c75"
                }
            },
            {
                "name": "Operators",
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#56b6c2"
                }
            },
            {
                "name": "Tags",
                "scope": [
                    "entity.name.tag",
                    "meta.tag.sgml"
                ],
                "settings": {
                    "foreground": "#e06c75"
                }
            },
            {
                "name": "Attributes",
                "scope": "entity.other.attribute-name",
                "settings": {
                    "foreground": "#d19a66"
                }
            },
            {
                "name": "Markup - Headings",
                "scope": [
                    "markup.heading",
                    "entity.name.section"
                ],
                "settings": {
                    "foreground": "#e06c75"
                }
            },
            {
                "name": "Markup - Bold",
                "scope": "markup.bold",
                "settings": {
                    "fontStyle": "bold",
                    "foreground": "#d19a66"
                }
            },
            {
                "name": "Markup - Italic",
                "scope": "markup.italic",
                "settings": {
                    "fontStyle": "italic",
                    "foreground": "#c678dd"
                }
            },
            {
                "name": "Markup - Links",
                "scope": [
                    "markup.underline.link",
                    "string.other.link"
                ],
                "settings": {
                    "foreground": "#61afef"
                }
            }
        ]
    },
